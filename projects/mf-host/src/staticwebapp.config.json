{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/mf-booking/*",
      "/mf-catalog/*",
      "/mf-company/*",
      "/mf-dashboard/*",
      "/mf-report/*",
      "/mf-user/*"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
